{
  "name": "@trezoaplex-foundation/tpl-token-metadata-kit",
  "version": "0.0.1",
  "description": "JavaScript client for Token Metadata using @trezoa/kit",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "build:docs": "typedoc",
    "test": "NODE_OPTIONS='--experimental-global-webcrypto' ava",
    "lint": "eslint --ext js,ts,tsx src",
    "lint:fix": "eslint --fix --ext js,ts,tsx src",
    "format": "prettier --check src test",
    "format:fix": "prettier --write src test"
  },
  "files": [
    "/dist/src"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://trezoaplex.com",
  "repository": "https://github.com/metaplex-foundation/tpl-token-metadata.git",
  "author": "Trezoaplex Maintainers <contact@trezoaplex.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@trezoa-program/token": "^0.9.0",
    "@types/node": "^20.0.0",
    "ava": "^5.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typedoc": "^0.28.0",
    "typedoc-plugin-missing-exports": "^4.0.0",
    "typescript": "^5.9.3",
    "vercel": "^28.16.8"
  },
  "ava": {
    "typescript": {
      "compile": false,
      "rewritePaths": {
        "src/": "dist/src/",
        "test/": "dist/test/"
      }
    },
    "require": [
      "./test/_ava-setup.js"
    ],
    "timeout": "2m",
    "serial": true
  },
  "packageManager": "pnpm@8.9.0",
  "dependencies": {
    "@trezoa-program/token": "^0.9.0",
    "@trezoa/accounts": "^5.0.0",
    "@trezoa/addresses": "^5.0.0",
    "@trezoa/codecs": "^5.0.0",
    "@trezoa/codecs-strings": "^5.1.0",
    "@trezoa/functional": "^5.1.0",
    "@trezoa/instructions": "^5.1.0",
    "@trezoa/keys": "^5.0.0",
    "@trezoa/kit": "^5.0.0",
    "@trezoa/rpc": "^5.0.0",
    "@trezoa/rpc-subscriptions": "^5.0.0",
    "@trezoa/rpc-types": "^5.0.0",
    "@trezoa/signers": "^5.0.0",
    "@trezoa/transaction-confirmation": "^2.0.0",
    "@trezoa/transaction-messages": "^5.0.0",
    "@trezoa/transactions": "^5.0.0"
  }
}
